        semanticui_visibility: {
            deps: ['jquery']
        },
        semanticui_modal:{
            deps: ['semanticui_transition','semanticui_dimmer'],
            exports: 'semanticui_modal'
        },
        jsonview:{
            deps: ['jquery']
        },
        "codemirror-showhint":{
          deps: ['codemirror']
        },
        "codemirror-lint":{
          deps: ['codemirror']
        },
        "codemirror-mode-hql":{
          deps: ['codemirror']
        },
        "codemirror-hint-hql":{
          deps: ["codemirror-showhint"]
        },
        "codemirror-lint-hql":{
          deps: ["codemirror-lint"]
        }
